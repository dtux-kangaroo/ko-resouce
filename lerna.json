{
    "lerna": "3.8.0",
    "workspaces": [
      "tools/*",
      "react-resource/blocks/*",
      "react-resource/layouts/*",
      "react-resource/components/*",
      "react-resource/samples/*",
      "react-resource/scaffolds/*",
      "vue-resource/blocks/*",
      "vue-resource/layouts/*",
      "vue-resource/scaffolds/*"
    ],
    "version": "independent",
    "npmClient": "npm",
    "registry": "http://registry.npmjs.org",
    "changelog": {
      "repo": "dtux/ko-create-app",
      "labels": {
        "tag: new feature": ":rocket: New Feature",
        "tag: breaking change": ":boom: Breaking Change",
        "tag: bug fix": ":bug: Bug Fix",
        "tag: enhancement": ":nail_care: Enhancement",
        "tag: documentation": ":memo: Documentation",
        "tag: internal": ":house: Internal",
        "tag: underlying tools": ":hammer: Underlying Tools"
      },
      "cacheDir": ".changelog"
    }
}
  